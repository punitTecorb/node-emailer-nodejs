{
  "name": "Send Mail",
  "version": "0.0.0",
  "scripts": {
    "build": "./node_modules/.bin/ts-node build.ts",
    "lint": "eslint . --ext .ts",
    "start": "node -r module-alias/register ./dist --env=development",
    "start:dev": "nodemon",
    "test": "mocha -r ts-node/register src/test/**/*_spec.ts",
    "test-debug": "mocha --debug 'test/**/*.js' --timeout 10000"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@controllers": "dist/controllers",
    "@utils": "dist/utils",
    "@server": "dist/server",
    "@routes": "dist/routes",
    "@models": "dist/models",
    "@validators": "dist/validators"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:@typescript-eslint/recommended-requiring-type-checking"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "rules": {
      "max-len": [
        "error",
        {
          "code": 100
        }
      ],
      "no-console": 1,
      "no-extra-boolean-cast": 0,
      "@typescript-eslint/restrict-plus-operands": 0,
      "@typescript-eslint/explicit-module-boundary-types": 0,
      "@typescript-eslint/no-explicit-any": 0,
      "@typescript-eslint/no-floating-promises": 0,
      "@typescript-eslint/no-unsafe-member-access": 0,
      "@typescript-eslint/no-unsafe-assignment": 0
    }
  },
  "eslintIgnore": [
    "src/public/",
    "build.ts"
  ],
  "dependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^5.0.0",
    "@types/chai-http": "^4.2.0",
    "@types/jasmine": "^4.3.1",
    "@types/request": "^2.48.8",
    "@types/supertest": "^2.0.12",
    "bcrypt": "^5.1.0",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cron": "^2.2.0",
    "crypto-js": "^4.1.1",
    "datepicker": "0.0.0",
    "decrypt": "^0.0.1",
    "delay": "^5.0.0",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "encrypt": "^0.0.1",
    "expo-firebase-recaptcha": "^2.2.0",
    "express": "^4.17.3",
    "express-async-errors": "^3.1.1",
    "fcm-node": "^1.3.0",
    "firebase": "^9.9.0",
    "fusioncharts": "^3.19.0",
    "geolib": "^3.3.3",
    "getdistance": "^1.0.0",
    "haversine": "^1.1.1",
    "haversine-distance": "^1.2.1",
    "helmet": "^5.0.2",
    "html-pdf": "^3.0.1",
    "http-status-codes": "^2.2.0",
    "jasmine-ts-console-reporter": "^3.1.1",
    "jet-logger": "^1.1.5",
    "joi": "^17.6.0",
    "jquery-fusioncharts": "^1.2.1",
    "js-datepicker": "^5.18.0",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "moment-range": "^4.0.2",
    "moment-timezone": "^0.5.34",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.1",
    "request-promise": "^4.2.6",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/chai-as-promised": "^7.1.5",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^9.0.13",
    "@types/jest": "^29.4.0",
    "@types/jquery": "^3.5.16",
    "@types/jsonfile": "^6.1.0",
    "@types/mocha": "^10.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.45",
    "@types/qrcode": "^1.5.0",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "axios": "^0.26.1",
    "chai": "^4.3.7",
    "eslint": "^8.11.0",
    "find": "^0.3.0",
    "fs-extra": "^10.0.1",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.6.4",
    "tsconfig-paths": "^3.14.0",
    "typescript": "^4.9.5",
    "typescript-module-alias": "^1.0.2"
  }
}
